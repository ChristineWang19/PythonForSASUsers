     "text": [
      "1         data source;\n",
      "2         do x=1 to 10;\n",
      "3         y =ranuni(-1);\n",
      "4         z =ranuni(1);\n",
      "5         output;\n",
      "6         end;\n",
      "7         run;\n",
      "\n",
      "\u001b[0;34mNOTE: Data set \"WORK.source\" has 10 observation(s) and 3 variable(s)\u001b[m\n",
      "\u001b[0;34mNOTE: The data step took :\u001b[m\n",
      "      real time : 0.943\n",
      "      cpu time  : 0.031\n",
      "\n",
      "\n",
      "8         proc python;\n",
      "9         export data=source;\n",
      "\u001b[0;34mNOTE: Creating Python data frame 'source' from data set 'WORK.source'\u001b[m\n",
      "\n",
      "10        submit;\n",
      "11        import statsmodels.formula.api as lm\n",
      "12        result = lm.ols(formula='x ~ y + z', data=source).fit()\n",
      "13        print (result.params)\n",
      "14        endsubmit;\n",
      "\n",
      "\u001b[0;34mNOTE: Submitting statements to Python:\u001b[m\n",
      "\n",
      "\n",
      "15        run;\n",
      "\u001b[0;34mNOTE: Procedure python step took :\u001b[m\n",
      "      real time : 56.282\n",
      "      cpu time  : 0.015\n",
      "\n",
      "\n"
     ]
